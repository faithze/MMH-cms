	<postSection_o item-list="true">	<item datasource="sections" inline="true">	<key>9bf42a7e-38c4-1d6c-03c7-689bce7b4886</key>
	<value>Intro</value>
<component id="9bf42a7e-38c4-1d6c-03c7-689bce7b4886">
	<content-type>/component/component-articlesection</content-type>
	<display-template>/templates/web/components/article-section.ftl</display-template>
